<h1>Listing published_issues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Volume</th>
    <th>Issue</th>
    <th>Pull date</th>
  </tr>

<% @published_issues.each do |published_issue| %>
  <tr>
    <td><%=h published_issue.name %></td>
    <td><%=h published_issue.volume %></td>
    <td><%=h published_issue.issue %></td>
    <td><%=h published_issue.pull_date %></td>
    <td><%= link_to 'Show', published_issue %></td>
    <td><%= link_to 'Edit', edit_published_issue_path(published_issue) %></td>
    <td><%= link_to 'Destroy', published_issue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New published_issue', new_published_issue_path %>