<h1>Listing shipping_addresses</h1>

<table>
  <tr>
    <th>Line1</th>
    <th>Line2</th>
    <th>City</th>
    <th>State</th>
    <th>Province</th>
    <th>Zip</th>
    <th>Country</th>
  </tr>

<% @shipping_addresses.each do |shipping_address| %>
  <tr>
    <td><%=h shipping_address.line1 %></td>
    <td><%=h shipping_address.line2 %></td>
    <td><%=h shipping_address.city %></td>
    <td><%=h shipping_address.state_id %></td>
    <td><%=h shipping_address.province %></td>
    <td><%=h shipping_address.zip %></td>
    <td><%=h shipping_address.country_id %></td>
    <td><%= link_to 'Show', shipping_address %></td>
    <td><%= link_to 'Edit', edit_shipping_address_path(shipping_address) %></td>
    <td><%= link_to 'Destroy', shipping_address, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New shipping_address', new_shipping_address_path %>