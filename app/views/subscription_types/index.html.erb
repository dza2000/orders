<h1>Listing subscription_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Number of issues</th>
    <th>Unit cost</th>
  </tr>

<% @subscription_types.each do |subscription_type| %>
  <tr>
    <td><%=h subscription_type.name %></td>
    <td><%=h subscription_type.number_of_issues %></td>
    <td><%=h subscription_type.unit_cost %></td>
    <td><%= link_to 'Show', subscription_type %></td>
    <td><%= link_to 'Edit', edit_subscription_type_path(subscription_type) %></td>
    <td><%= link_to 'Destroy', subscription_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New subscription_type', new_subscription_type_path %>