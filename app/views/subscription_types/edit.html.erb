<h1>Editing subscription_type</h1>

<% form_for(@subscription_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :number_of_issues %><br />
    <%= f.text_field :number_of_issues %>
  </p>
  <p>
    <%= f.label :unit_cost %><br />
    <%= f.text_field :unit_cost %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @subscription_type %> |
<%= link_to 'Back', subscription_types_path %>