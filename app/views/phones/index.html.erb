<h1>Listing phones</h1>

<table>
  <tr>
    <th>Prefix</th>
    <th>Phone number</th>
    <th>Extension</th>
    <th>Type</th>
    <th>Is primary</th>
    <th>Contact id</th>
  </tr>

<% @phones.each do |phone| %>
  <tr>
    <td><%=h phone.prefix %></td>
    <td><%=h phone.phone_number %></td>
    <td><%=h phone.extension %></td>
    <td><%=h phone.type %></td>
    <td><%=h phone.is_primary %></td>
    <td><%=h phone.contact_ID %></td>
    <td><%= link_to 'Show', phone %></td>
    <td><%= link_to 'Edit', edit_phone_path(phone) %></td>
    <td><%= link_to 'Destroy', phone, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New phone', new_phone_path %>