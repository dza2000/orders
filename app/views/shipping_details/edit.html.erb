<h1>Editing shipping_detail</h1>

<% form_for(@shipping_detail) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :transaction_ID %><br />
    <%= f.text_field :transaction_ID %>
  </p>
  <p>
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.label :address_line1 %><br />
