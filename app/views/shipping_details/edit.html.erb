  </p>
  <p>
    <%= f.label :address_line2 %><br />
    <%= f.text_field :address_line2 %>
  </p>
  <p>
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :state_ID %><br />
    <%= f.text_field :state_ID %>
  </p>
  <p>
    <%= f.label :province %><br />
    <%= f.text_field :province %>
  </p>
  <p>
    <%= f.label :country_ID %><br />
    <%= f.text_field :country_ID %>
  </p>
  <p>
    <%= f.label :shipping_method %><br />
    <%= f.text_field :shipping_method %>
