<div id="transactions">	<h4>Customer Transactions</h4><table width=600 border="1"><tr><td>Transaction ID</td><td>Estore Purchase</td><td>Journal Subscription</td><td>Donation</td><td>Transaction Date</td><td>Transaction Amount</td></tr><% for transaction in @contact.transactions %><tr><td><%= link_to transaction.id, transaction %></td><td><% if transaction.orders.size >= 1 %><center><%= image_tag('check.png', :width=>'35px')%></center><% else %><% end %></td><td><% if transaction.subscriptions.size >= 1 %><center><%= image_tag('check.png', :width=>"35px")%><% else %><% end %></center></td><td><% if transaction.donations.size >= 1 %><center><%= image_tag('check.png', :width=>'35px')%><% else %><% end %></center></td><td><%= transaction.created_at.strftime("%m-%d-%Y")%></td><td><%= number_to_currency(transaction.practice) %></td></tr><% end %></table></div>