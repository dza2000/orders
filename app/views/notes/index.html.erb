<h1>Listing notes</h1>

<table>
  <tr>
    <th>Subject</th>
    <th>Body</th>
    <th>Staff</th>
    <th>Contact</th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%=h note.subject %></td>
    <td><%=h note.body %></td>
    <td><%=h note.staff_id %></td>
    <td><%=h note.contact_id %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New note', new_note_path %>