<h1>Donation Lines</h1>

<% form_for :dump, :url=>{:controller=>"donation_lines", :action=>"csv_import"}, :html => { :multipart => true } do |f| -%>
 <table>
   <tr>
     <td>
      <label for="dump_file">
        <b>Import a CSV File :</b>
      </label>
     </td>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end -%>

<%= link_to 'New donation_line', new_donation_line_path %>